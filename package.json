{
    "name": "vue3-components-leaflet",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-s clean build:dts build:lib",
        "build:dts": "vue-tsc -b --force tsconfig.dts.json",
        "build:pre": "rollup --config build/rollup.config.ts --configPlugin typescript",
        "build:lib": "npm run build:pre && rollup --config dist/rollup.config.mjs",
        "preview": "vite preview",
        "clean": "rimraf dist/"
    },
    "dependencies": {
        "vue": "^3.5.22"
    },
    "devDependencies": {
        "@rollup/plugin-replace": "^6.0.2",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.3.0",
        "@types/clean-css": "^4.2.11",
        "@types/leaflet": "^1.9.21",
        "@types/leaflet.markercluster": "^1.5.6",
        "@types/node": "^24.6.0",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/tsconfig": "^0.8.1",
        "clean-css": "^5.3.3",
        "deepmerge": "^4.3.1",
        "npm-run-all2": "^8.0.4",
        "rimraf": "^6.0.1",
        "rollup": "^4.52.5",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-dts": "^6.2.3",
        "rollup-plugin-esbuild": "^6.2.1",
        "rollup-plugin-postcss": "^4.0.2",
        "sass": "^1.93.2",
        "typescript": "~5.9.3",
        "vite": "^7.1.7",
        "vite-plugin-external": "^6.2.2",
        "vue-tsc": "^3.1.0"
    }
}
